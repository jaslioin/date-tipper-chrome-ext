{
  "name": "Date Tipper",
  "description": "Tips about what kind of date today is, don't get bashed by your gf for not remembering what date it is!",
  "version": "0.1",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "How special is it today?",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
